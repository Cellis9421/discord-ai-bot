{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": "off",
      "max-len": ["error", 150],
      "global-require": "off",
      "no-underscore-dangle": "off",
      "radix": "off"
    }
}